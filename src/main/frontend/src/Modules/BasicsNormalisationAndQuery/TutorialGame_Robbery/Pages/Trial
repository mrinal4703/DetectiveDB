    const [isTyping, setIsTyping] = useState(false);

    const steps = [
        {
            image: selectsql,
            text: AppText.SelectSQL,
            position: 'left',
            initialScale: 0.616,
            initialX: 0,
            initialY: 100,
            finalX: 0,
